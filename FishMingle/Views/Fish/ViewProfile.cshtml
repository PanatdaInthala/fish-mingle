@using FishMingle.Models;
@{
  Layout = "_Layout";
}
<nav class="navbar navbar-expand-md navbar-dark bg-dark">
<img class="fishlovers" src="~/img/fishlovers.png">
  <a class="navbar-brand" href="/">Fish Mingle</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarText">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <a class="nav-link" href="/">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/fish/@Model["newFish"].GetId()">Profile</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/users/browse">Browse</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/user/matches">Matches</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/user/logout">Logout</a>
      </li>
    </ul>
    <span class="navbar-text">
    </span>
  </div>
</nav>
<div class="container">
@if(Model!=null)
{
<h1>Profile: @Model["newFish"].GetName()</h1>
  <h3>Your matches:</h3>
  foreach (var user in Model["matchList"])
  {
    <h4><li>@user.GetName()</li></h4>
  }


}
<h1>@Model["sessionId"]</h1>
      <button type="submit" name="button"></button>
      <button> <a href="/fish/@Model["sessionId"]/update/password"
      class="btn">update your password</a></button>
      <button> <a href="/fish/@Model["newFish"].GetId()/update/username"
       class="btn">update your personal information</a></button>

<hr>
<footer id="footer">FishMingle.com Â©2018 </footer>
</div>
